{
  "name": "motters-monorepo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn workspace @motters/web dev",
    "build": "yarn workspace @motters/web build",
    "lint": "yarn workspace @motters/web lint",
    "preview": "yarn workspace @motters/web preview",
    "dev:all": "yarn workspaces run dev",
    "build:all": "yarn workspaces run build",
    "lint:all": "yarn workspaces run lint",
    "workspaces:info": "yarn workspaces info"
  },
  "devDependencies": {
    "prettier": "3.3.3"
  },
  "packageManager": "yarn@4.0.0"
}
