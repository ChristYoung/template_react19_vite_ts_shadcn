# Ignore node_modules
node_modules/

# Ignore build folder
dist/
build/

# Ignore environment configuration files
.env
.env.local
.env.*.local

# Ignore lock file
package-lock.json
yarn.lock

# Ignore Vite configuration file (if you don't want it to be formatted by Prettier)
vite.config.js

# Ignore all generated files
*.log
*.min.js
*.min.css

# Ignore formatting of specific file types
*.svg
*.ico
*.png
*.jpg
*.jpeg
